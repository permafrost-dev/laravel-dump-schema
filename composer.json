{
    "name": "permafrost-dev/laravel-dump-schema",
    "description": "Implementation of database schema dumping and loading to/from file, taken from Laravel v8 PR",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "illuminate/console": "^6 - ^7",
        "illuminate/support": "^6 - ^7",
        "symfony/process": "^4 - ^5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Permafrost\\LaravelDumpSchema\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Permafrost\\LaravelDumpSchema\\LaravelDumpSchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}